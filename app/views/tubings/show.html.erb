<div class="page-header">
  <h3><%= @tubing.name %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Coupling</dt>
      <dd><%= @tubing.coupling.description %></dd>

      <dt>Packer</dt>
      <dd><%= @tubing.packer.description %></dd>

      <dt>Safety valve</dt>
      <dd><%= @tubing.safety_valve.description %></dd>

      <dt>Name</dt>
      <dd><%= @tubing.name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/tubings" class="btn btn-primary">
        Back
      </a>
      <a href="/tubings/<%= @tubing.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_tubing/<%= @tubing.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
